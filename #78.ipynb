    "# 78. Subsets\n",
    "class Solution:\n",
    "    def subsets(self, nums):\n",
    "        result = [[]]\n",
    "        for num in nums:\n",
    "            result += [i + [num] for i in result]\n",
    "            print(result)\n",
    "        return result\n",
    "    \n",
    "s = Solution()\n",
    "s.subsets([1,2,3])"
   ]
