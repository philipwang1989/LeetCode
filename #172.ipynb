    "# 172. Factorial Trailing Zeroes\n",
    "class Solution(object):\n",
    "    def trailingZeroes(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        count = 0\n",
    "        while n > 0:\n",
    "            n = n/5\n",
    "            count += n\n",
    "            \n",
    "        return count"
   ]
