    "# 1217. Play with Chips\n",
    "class Solution:\n",
    "    def minCostToMoveChips(self, chips: List[int]) -> int:\n",
    "        d = collections.Counter([c % 2 for c in chips])\n",
    "        return min(d[0], d[1])"
